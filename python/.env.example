# Configuração do banco de dados (mesmo do sistema TypeScript)
# Use a mesma URL do banco que está configurada no sistema TypeScript
DATABASE_URL=postgresql://username:password@localhost:5432/biometric_access_system

# Configuração do sensor R307
SENSOR_DEVICE=R307
SENSOR_PORT=/dev/ttyUSB0
SENSOR_BAUDRATE=57600

# Configuração de logs
LOG_LEVEL=INFO
LOG_FILE=biometric_query.log

# Configuração da unidade escolar (código da unidade onde o sensor está instalado)
DEFAULT_UNIT_CODE=ETEC01

